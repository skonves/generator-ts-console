{
  "name": "generator-ts-console",
  "version": "1.5.0",
  "description": "Generator for basic Typescript applications",
  "main": "./generators/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "clean": "run-s -s clean:*",
    "clean:output": "rimraf generators",
    "clean:coverage": "rimraf coverage",
    "prebuild": "run-s -s clean lint",
    "build": "tsc",
    "postbuild": "copyfiles -a -u 1 \"src/*/templates/**/*\" generators",
    "lint": "run-s -s lint:*",
    "lint:eslint": "eslint src/**/*.*",
    "lint:prettier": "prettier -c .",
    "fix": "run-s -s fix:*",
    "fix:eslint": "eslint --fix src/**/*.*",
    "fix:prettier": "prettier -w .",
    "prepack": "npm run build && copyfiles .npmignore generators",
    "start": "node ./generators/index.js",
    "pretest": "run-s -s clean",
    "test": "jest",
    "dev": "npm uninstall -g generator-ts-console && npm run build && npm i -g ."
  },
  "keywords": [
    "yeoman",
    "generator",
    "typescript",
    "eslint",
    "prettier",
    "jest"
  ],
  "author": "Steve Konves",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skonves/generator-ts-console.git"
  },
  "bugs": {
    "url": "https://github.com/skonves/generator-ts-console/issues"
  },
  "homepage": "https://github.com/skonves/generator-ts-console#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/rimraf": "^3.0.2",
    "@types/vinyl": "^2.0.6",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^3.1.3",
    "@types/yeoman-test": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^5.3.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "dependencies": {
    "generator-license": "^5.5.0",
    "gulp-prettier-plugin": "^1.3.0",
    "prettier": "^2.4.1",
    "vinyl": "^2.2.1",
    "yeoman-generator": "^4.12.0"
  }
}
