{
  "name": "generator-ts-console",
  "version": "0.3.1",
  "description": "Generator for basic Typescript applications",
  "main": "./lib/index.js",
  "scripts": {
    "prebuild": "npm run lint && rimraf generators",
    "build": "tsc",
    "postbuild": "copyfiles -a -u 1 \"src/*/templates/**/*\" generators",
    "lint": "tslint -c tslint.json -e 'node_modules/**/*' '**/*.ts'",
    "prepack": "npm run build",
    "start": "node ./lib/index.js",
    "pretest": "rm -rf coverage/*",
    "test": "jest",
    "dev": "npm uninstall -g generator-ts-console && npm run build && npm i -g ."
  },
  "keywords": [],
  "author": "Steve Konves",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.1",
    "@types/rimraf": "^2.0.2",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^3.1.3",
    "@types/yeoman-test": "^2.0.3",
    "copyfiles": "^2.3.0",
    "generator-license": "^5.4.0",
    "jest": "^26.6.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^26.4.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^4.0.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^4.0.1"
  }
}
