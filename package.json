{
  "name": "generator-ts-console",
  "version": "0.3.1",
  "description": "Generator for basic Typescript applications",
  "main": "./lib/index.js",
  "scripts": {
    "prebuild": "npm run lint && rimraf generators",
    "build": "tsc",
    "postbuild": "copyfiles -a -u 1 \"src/*/templates/**/*\" generators",
    "lint": "tslint -c tslint.json -e 'node_modules/**/*' '**/*.ts'",
    "prepack": "npm run build",
    "start": "node ./lib/index.js",
    "pretest": "npm run build",
    "test": "NODE_ENV=test nyc mocha --require source-map-support/register --require ts-node/register --recursive './src/**/*.tests.ts'",
    "dev": "npm uninstall -g generator-ts-console && npm run build && npm i -g ."
  },
  "keywords": [],
  "author": "Steve Konves",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.12",
    "@types/rimraf": "^2.0.2",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^3.1.3",
    "@types/yeoman-test": "^2.0.3",
    "chai": "^4.2.0",
    "copyfiles": "^2.3.0",
    "generator-license": "^5.4.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^4.0.1"
  }
}
